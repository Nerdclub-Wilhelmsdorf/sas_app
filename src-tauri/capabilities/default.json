{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    "store:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://banking.saswdorf.de" }]
    }
  ]
}